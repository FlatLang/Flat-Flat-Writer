package flat/writer/flat

import flat/ast
import flat/compiler/models/Token
import flat/io
import flat/log/Logger
import flat/eventstream/EventStream
import flat/writer

class extends NodeWriter<ImportNode> {
    public write(ImportNode node, WriterOutputStream output) {
        output.writeString("import")

        if (node.type) {
            output.writeString(" #{node.type.value}")
        }

        output.writeString(node.location)
    }
}
