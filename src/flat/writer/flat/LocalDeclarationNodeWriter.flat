package flat/writer/flat

import flat/ast
import flat/compiler/models/Token
import flat/io
import flat/log/Logger
import flat/eventstream/EventStream
import flat/writer

class extends NodeWriter<LocalDeclarationNode> {
  public write(LocalDeclarationNode node, WriterOutputStream output) {
    if (node.type) {
      output.write(node.type)
    }

    for (i in 0..node.arrayDimensions) {
      output.write("[]")
    }

    output.write(node.name)
  }
}
